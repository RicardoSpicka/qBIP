<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fonoaudiologia.qbip">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_qbip"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QBIP">
        <activity
            android:name=".Main_Activity"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".freq_game"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name=".dur_game"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape"></activity>

        <activity
            android:name=".vol_game"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape"></activity>

        <activity
            android:name=".inter_game"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape"></activity>

        <activity
            android:name=".conf_freq_game"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape"></activity>

        <activity
            android:name=".conf_dur_game"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape"></activity>

        <activity
            android:name=".conf_vol_game"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape"></activity>

        <activity
            android:name=".conf_inter_game"
            android:configChanges="orientation|keyboard|keyboardHidden"
            android:screenOrientation="landscape"></activity>


    </application>

</manifest>